{
    "name": "Homegear",
    "version": "0.8.x",
    "slug": "homegear",
    "description": "Homegear nightly as a Hassio add-on",
    "startup": "before",
    "boot": "auto",
    "gpio": true,
    "ports": {
        "2001/tcp": 2001
    },
    "ports_description": {
        "2001/tcp": "HomeGear UI"
    },
    "privileged": [
        "SYS_ADMIN",
        "SYS_RAWIO"
    ],
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "map": [
        "config:rw",
        "share:rw"
    ],
    "host_network": true,
    "environment": {
    },
    "options": {
    },
    "schema": {
    },
    "devices": [
        "/dev/ttyUSB0:/dev/ttyUSB0:rwm"
    ]
}
